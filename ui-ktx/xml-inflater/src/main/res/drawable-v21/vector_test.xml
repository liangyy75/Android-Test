<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:name="@android:string/cancel"
    android:width="@android:dimen/app_icon_size"
    android:height="@android:dimen/app_icon_size"
    android:alpha="@android:integer/config_shortAnimTime"
    android:autoMirrored="true"
    android:opticalInsetLeft="@android:dimen/app_icon_size"
    android:opticalInsetTop="@android:dimen/app_icon_size"
    android:opticalInsetRight="@android:dimen/app_icon_size"
    android:opticalInsetBottom="@android:dimen/app_icon_size"
    android:tint="@android:color/transparent"
    android:tintMode="src_over"
    android:viewportWidth="@android:integer/config_shortAnimTime"
    android:viewportHeight="@android:integer/config_shortAnimTime">
    <clip-path
        android:name="@android:string/cancel"
        android:pathData="@android:string/cancel" />
    <group
        android:name="@android:string/cancel"
        android:pivotX="@android:integer/config_shortAnimTime"
        android:pivotY="@android:integer/config_shortAnimTime"
        android:rotation="@android:integer/config_shortAnimTime"
        android:scaleX="@android:integer/config_shortAnimTime"
        android:scaleY="@android:integer/config_shortAnimTime"
        android:translateX="@android:integer/config_shortAnimTime"
        android:translateY="@android:integer/config_shortAnimTime">
        <path
            android:name="@android:string/cancel"
            android:fillAlpha="@android:integer/config_shortAnimTime"
            android:fillColor="@android:color/transparent"
            android:fillType="nonZero"
            android:pathData="@android:string/cancel"
            android:strokeWidth="@android:integer/config_shortAnimTime"
            android:strokeAlpha="@android:integer/config_shortAnimTime"
            android:strokeColor="@android:color/transparent"
            android:strokeLineCap="square"
            android:strokeLineJoin="miter"
            android:strokeMiterLimit="@android:integer/config_shortAnimTime"
            android:trimPathStart="@android:integer/config_shortAnimTime"
            android:trimPathEnd="@android:integer/config_shortAnimTime"
            android:trimPathOffset="@android:integer/config_shortAnimTime" />
    </group>
</vector>
