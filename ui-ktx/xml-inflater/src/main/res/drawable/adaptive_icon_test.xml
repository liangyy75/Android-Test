<?xml version="1.0" encoding="utf-8"?>
<adaptive-icon xmlns:android="http://schemas.android.com/apk/res/android"
    android:drawable="@android:color/transparent">
    <background
        android:id="@+id/hahaha"
        android:width="@android:dimen/app_icon_size"
        android:height="@android:dimen/app_icon_size"
        android:bottom="@android:dimen/app_icon_size"
        android:drawable="@android:color/transparent"
        android:end="@android:dimen/app_icon_size"
        android:gravity="center_vertical"
        android:left="@android:dimen/app_icon_size"
        android:right="@android:dimen/app_icon_size"
        android:start="@android:dimen/app_icon_size"
        android:top="@android:dimen/app_icon_size">
        <scale />
    </background>
    <foreground
        android:id="@+id/xixixi"
        android:width="@android:dimen/app_icon_size"
        android:height="@android:dimen/app_icon_size"
        android:bottom="@android:dimen/app_icon_size"
        android:drawable="@android:color/transparent"
        android:end="@android:dimen/app_icon_size"
        android:gravity="start"
        android:left="@android:dimen/app_icon_size"
        android:right="@android:dimen/app_icon_size"
        android:start="@android:dimen/app_icon_size"
        android:top="@android:dimen/app_icon_size">
        <shape />
    </foreground>
</adaptive-icon>