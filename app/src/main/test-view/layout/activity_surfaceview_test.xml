<?xml version="1.0" encoding="UTF-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/test_surfaceview_float_button"
        android:layout_width="@dimen/dimen50"
        android:layout_height="@dimen/dimen50"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/dimen20"
        app:srcCompat="@drawable/icon_left" />

    <com.liang.example.viewtest.surfaceview.SurfaceViewTest
        android:id="@+id/test_surfaceview_surfaceview1"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.liang.example.viewtest.surfaceview.SurfaceViewTest
        android:id="@+id/test_surfaceview_surfaceview2"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</FrameLayout>
